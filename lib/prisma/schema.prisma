// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Stock{
  id Int @id @default(autoincrement())
  stock String @unique
  price String
  quantity Int
  date DateTime @default(now())
  memo String
}

